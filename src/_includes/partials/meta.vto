<meta charset="utf-8">
{{for name,content of metaTags }}
    <meta name="{{name}}" content="{{content |> exec}}">
{{ /for }}
{{ if description }}
    <meta name="description" content="{{description |> escape}}">
{{ /if }}
{{ if noindex }}
    <meta name="robots" content="noindex">
{{ /if }}
<title>{{title}}</title>
<link rel="preload" as="font" type="font/woff2" href="/crimson.regular.woff2" crossorigin>
<link rel="preload" as="font" type="font/woff2" href="/crimson.bold.woff2" crossorigin>
<link rel="stylesheet" href="/main.css">
{{ if isContentPage }}
    {{ if config.analytics }}
        <script defer src="/infoot.js" defer></script>
    {{ /if }}
{{ /if }}
<script src="/main.js" defer></script>
{{ if config.analytics && process.env["ELEVENTY_ENV"] == "production" }}
    {{config.analytics.analytics }}
{{ /if }}
{{ if headInject }}{{ headInject }}{{ /if }}
